    <ProjectReference Include="..\..\..\GCModeller\analysis\SequenceToolkit\MSA\MSA.NET5.vbproj" />
    <ProjectReference Include="..\..\..\GCModeller\analysis\SequenceToolkit\SequenceLogo\SequenceLogo.NET5.vbproj" />
