# query of the kegg map for the data visualization
# of the pathway enrichment result

map css("area", "*") [{
	
	id          attr("id")
	shape       attr("shape")
	coords      attr("coords")
	data_coords attr("data-coords")
	class       attr("class")
	href        attr("href") | replace("javascript:void(0)", "")
	title       attr("title")
	entry       attr("data-entry")
	refid       attr("data-refid")
	module      attr("data-module")
	
}]