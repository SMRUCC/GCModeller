    <Title>Assembly file I/O stream for the common biological database and common biological data models</Title>
    <Description>Bio.Assembly: assembly file i/o stream for the common biological database and the common biological data models. This is the core library for the genome annotation in GCModeller.</Description>
    <Product>GCModeller</Product>
    <Company>Southern Microbial Resources Utilization Centre Of China</Company>
    <Authors>SMRUCC genomics</Authors>
    <Version>6.3.256.3655</Version>
    <Copyright>Copyright Â© SMRUCC genomics 2023</Copyright>
    <PackageProjectUrl>https://gcmodeller.org/</PackageProjectUrl>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/SMRUCC/GCModeller.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>gcmodeller;bioinformatics;genomics</PackageTags>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

